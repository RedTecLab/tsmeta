{
  "basePackage": "package.json",
  "metaConfig": {
    "create": true,
    "outputPath": "schema",
    "outputFilename": "tsmeta_schema.json",
    "compilerOptions": "tsconfig.build.json"
  },
  "sigmaConfig": {
    "create": true,
    "outputPath": "schema",
    "outputFilename": "sigma_data.json",
    "createNodes": {
      "packages": true,
      "classes": true,
      "interfaces": true,
      "methods": false,
      "properties": false
    }
  },
  "oasConfig": {
    "create": true,
    "outputPath": "schema",
    "outputFilename": "oas_schema.json",
    "openapistring": "3.0.0",
    "annotationsMap": {
      "Controller": null,
      "Model": null,
      "GetRequest": null,
      "PostRequest": null,
      "PutRequest": null,
      "PatchRequest": null,
      "DeleteRequest": null,
      "HeadRequest": null,
      "SuccessResponse": null,
      "ErrorResponse": null,
      "PathVariable": null,
      "ReqParam": null,
      "ReqBody": null
    }

  },
  "graphQLConfig": {
    "create": true,
    "outputPath": "schema",
    "model_annotation": "Model",
    "property_annotation": "Property"
  }
}
