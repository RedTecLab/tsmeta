{
  "name": "tsmeta",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "version": "0.8.9",
  "description": "typescript project meta analysis",
  "keywords": [
    "typescript",
    "schema",
    "openapi",
    "graphql",
    "sigmajs",
    "annotation",
    "decorator"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/mafh612/tsmeta"
  },
  "scripts": {
    "prepublish": "yarn build",
    "test": "jest --config jest.config.js",
    "prebuild": "tslint --project tsconfig.build.json",
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn run build && node dist/bin.js",
    "start": "node dist/bin.js",
    "view": "node view/server.js",
    "watch": "yarn run prebuild && tsc --project tsconfig.build.json --watch"
  },
  "author": "mafh",
  "license": "MIT",
  "dependencies": {
    "deep-assign": "^3.0.0",
    "graphql": "^14.1.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "oasmodel": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "semantic-release": "^15.5.0",
    "typescript": "^3.3.3333",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/deep-assign": "^0.1.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.9",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.32",
    "@types/lodash": "^4.14.109",
    "@types/node": "^11.9.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/yamljs": "^0.2.30",
    "jest": "^24.1.0",
    "koa": "^2.6.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^6.0.0"
  },
  "release": {
    "analyzeCommits": [
      "@semantic-release/commit-analyzer"
    ],
    "branch": "master",
    "generateNotes": [
      "@semantic-release/release-notes-generator"
    ],
    "githubUrl": "https://github.com/mafh612/tsmeta",
    "noCi": true,
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "verifyRelease": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
