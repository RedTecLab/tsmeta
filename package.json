    "reflect-metadata": "^0.1.12",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.1",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.0",
    "@types/reflect-metadata": "^0.1.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3"
  }
